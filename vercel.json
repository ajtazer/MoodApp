{
	"builds": [
		{
		"src": "MoodApp/wsgi.py",
		"use": "@vercel/python",
		"config": {"maxLambdaSize": "50mb", "runtime": "python3.9"}
	},
	{
		"src": "build_files.sh",
		"use": "@vercel/static-build",
		"config":{
			"distDir": "prod_staticfiles"
		}
	}],
	"routes": [
		{
		  "src": "/static/(.*)",
		  "dest": "/static/$1"
		},
		{
		  "src": "/(.*)",
		  "dest": "projectname/wsgi.py"
		}
	  ]
	}